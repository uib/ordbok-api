<!ENTITY % Entitetar SYSTEM "bokmaalsordbok.ent">

%Entitetar;

<!ELEMENT band5 (#PCDATA)*>
<!ELEMENT STED (#PCDATA | SUP)*>
<!ATTLIST STED DS CDATA #IMPLIED>
<!ATTLIST STED S CDATA #IMPLIED>
<!ATTLIST STED V CDATA #IMPLIED>
<!ELEMENT TYPE (#PCDATA)>
<!ELEMENT DEFTEKST (#PCDATA | KIL | K | SUP| TILV | SMS | HERTIL | FRAK | FYL)*>
<!ELEMENT BOYPK (#PCDATA | BOY | FYL | GRM_FS)*>
<!ELEMENT BOY (#PCDATA)>
<!ELEMENT DELDEF (#PCDATA | DEFTEKST |FYL)*>
<!ELEMENT INNL (#PCDATA)>
<!ELEMENT GRM (#PCDATA)>
<!ELEMENT GRM_FS (#PCDATA)>
<!ELEMENT OPP_ORD_PAKKE (#PCDATA | OPPORD | K)*>
<!ELEMENT HAK (#PCDATA | K | KIL | SUP | TILV | VOK | FYL)*>
<!ELEMENT HERTIL (#PCDATA | INNL | REKKE | FYL)*>
<!ELEMENT FYL (#PCDATA)>
<!ELEMENT FRAK (#PCDATA)>
<!ELEMENT DOEMEREKKE (#PCDATA | DOEME | FYL)*>
<!ELEMENT DOEME (#PCDATA | K | N | KIL | HERTIL | FRAK | FORK|SMS|TILV | P | FYL)*>
<!ELEMENT KIL (#PCDATA | REFLISTE | REF | TILV | SUP |K|FYL)*>
<!ELEMENT FORF (#PCDATA)>
<!ELEMENT REKKE (#PCDATA | K|KIL)*>
<!ELEMENT REFLISTE (#PCDATA | DELREF|FYL)*>
